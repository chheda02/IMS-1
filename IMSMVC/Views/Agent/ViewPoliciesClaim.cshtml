@model IEnumerable<IMSMVC.Models.PoliciesClaim>

@{
    ViewBag.Title = "ViewPoliciesClaim";
    Layout = "~/Views/Shared/_LayoutAgent.cshtml";
}

<h2>ViewPoliciesClaim</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PolicyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequiredDocuments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimStatusID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reason)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PolicyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClaimAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequiredDocuments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClaimStatusID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reason)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
    </tr>
}

</table>
